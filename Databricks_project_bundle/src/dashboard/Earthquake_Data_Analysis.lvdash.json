{
  "datasets": [
    {
      "name": "95b24f84",
      "displayName": "Earthquake_Detail",
      "queryLines": [
        "Select * from catalog_dev.silver_dev.earthquake_data_final"
      ]
    },
    {
      "name": "528c0401",
      "displayName": "Earthquake_Magnitude_Type",
      "queryLines": [
        "select \r\n",
        "\tcount(*) as total_magnitude_bucket,\r\n",
        "    magnitude_bucket \r\n",
        "        from (\r\n",
        "            select \r\n",
        "                CASE\r\n",
        "                    WHEN mag < 2 THEN 'Micro (<2)'\r\n",
        "                    WHEN mag BETWEEN 2 AND 3 THEN 'Minor (2–3)'\r\n",
        "                    WHEN mag BETWEEN 3 AND 4 THEN 'Light (3–4)'\r\n",
        "                    ELSE 'Moderate+ (4+)'\r\n",
        "\t            END AS magnitude_bucket\r\n",
        "            from catalog_dev.silver_dev.earthquake_data_final )\r\n",
        "group by magnitude_bucket"
      ]
    },
    {
      "name": "c51b100a",
      "displayName": "Earthquake_By_Location",
      "queryLines": [
        "SELECT \r\n",
        "\tcount(*) AS event_count,\r\n",
        "\tsplit(place, ', ')[1] as Location\r\n",
        "FROM catalog_dev.silver_dev.earthquake_data_final\r\n",
        "GROUP BY Location"
      ]
    }
  ],
